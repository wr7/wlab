#![declare_crate(test)]

fn main() {
	if true {
		std::println("a");
	} else {
		std::println("unreachable");
	}

	if (if false {false} else {true}) {
		std::println("b");
	} else {
		std::println("unreachable");
	}

	if false {
		std::println("unreachable");
	} else if false {
		std::println("unreachable");
	} else {
