#![declare_crate(a)]

fn println(val: str) {
    write(1, val);
    write(1, "\n");
}

#[no_mangle]
fn _start() {
    println("what's 9 + 10?");

    if 9 + 10 == 21 {
        println("21");
    } else {
        println("19");
    }

    let x = if 9 + 10 == 21 {"goteem"} else {":("};

    println(x);

    exit(0);
}
